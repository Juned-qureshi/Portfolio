{
    "builds": [
        {
            "src": "portfolio/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python 3.10.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "portfolio/wsgi.py"
        }
    ]
}